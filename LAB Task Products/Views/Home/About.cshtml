@model List<LabTaskPractice.Models.Entity.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class=" table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Quantity</th>
        <th>Description</th>
        <th>Action</th>
    </tr>
    <asp:Button runat="server" Text="Button"></asp:Button>
    @foreach (var s in Model)
    {
        <tr>
            <td> @s.Id</td>
            <td> @s.Name</td>
            <td> @s.Price</td>
            <td> @s.Quantity</td>
            <td> @s.Description</td>


        </tr>


    }
</table>
